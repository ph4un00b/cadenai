{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"eslint.rules.customizations": [{ "rule": "*", "severity": "warn" }],
	"typescript.tsdk": "node_modules/typescript/lib",
	"cSpell.words": [
		"arktype",
		"arrappend",
		"cadenai",
		"clsx",
		"datetime",
		"Dont",
		"fau's",
		"faunicolas",
		"futuro",
		"hola",
		"ianvs",
		"jamon",
		"langchain",
		"llms",
		"lpush",
		"lrange",
		"manypkg",
		"mundo",
		"NEXTAUTH",
		"nextjs",
		"openai",
		"phau",
		"phaubonacci",
		"phaudecrypter",
		"phauencrypter",
		"planetscale",
		"pscale",
		"shadcn",
		"superjson",
		"suscrito",
		"tailwindcss",
		"tanstack",
		"Texto",
		"textos",
		"TIMEDIFF",
		"todos",
		"TRPC",
		"upstash",
		"waku",
		"zact"
	],
	"workbench.colorCustomizations": {
		"activityBar.background": "#3d2805",
		"titleBar.activeBackground": "#5e2b03",
		"titleBar.activeForeground": "#FCFAFE"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"conventionalCommits.scopes": [
		"nextjs",
		"chat-models",
		"embeddings",
		"waku-pdf"
	],
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
