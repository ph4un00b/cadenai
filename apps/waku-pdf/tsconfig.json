{
	// "extends": "../../tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"target": "esnext",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "nodenext",
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true,
		"exactOptionalPropertyTypes": true,
		"jsx": "react-jsx",
		"paths": {
			"~/*": ["./src/*"]
		}
	}
	// "include": [".eslintrc.js", "prettier.config.cjs"]
}
